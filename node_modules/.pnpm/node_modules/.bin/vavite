#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaoyu/Desktop/xiangmu/newbit-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaoyu/Desktop/xiangmu/newbit-web/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir//bin/env" ]; then
  exec "$basedir//bin/env"  node "$basedir/../@vavite/multibuild-cli/cli.js" "$@"
else
  exec /bin/env  node "$basedir/../@vavite/multibuild-cli/cli.js" "$@"
fi
